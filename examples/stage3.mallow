define even? as
    lambda x . x mod 2 = 0
end

define sum as
    lambda x . lambda y . x + y
end

puts even? | (sum | 10 ) | 20
puts even?| 5

define fact as
    lambda x . if x = 0 then 1 else x * fact | (x-1)
end

puts fact | 10

define fib as
    lambda x . if x < 2 then x else (fib | x-1) + (fib | x-2)
end


puts "Hello, World"
puts fib | 10
puts fib | 20

puts "Hello, World"

